{
  "name": "rural-destruction",
  "version": "0.1.0",
  "description": "The Google Sheets history erasing service for TableJet",
  "main": "src/index.ts",
  "scripts": {
    "exec-dev": "ts-node src/index.ts",

    "build": "tsc",
    "start": "node dist/index.js",

    "pm2-up": "npm run build && pm2 start dist/index.js --name tbj-rural-destruction",
    "pm2-down": "pm2 stop tbj-rural-destruction",

    "oneshot": "ts-node src/index.ts -- --ONESHOT"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YourBCABus/tbj-rural-destruction.git"
  },
  "author": "Yenowa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YourBCABus/tbj-rural-destruction/issues"
  },
  "homepage": "https://github.com/YourBCABus/tbj-rural-destruction#readme",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.0",
    "@types/node": "^20.6.0",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
